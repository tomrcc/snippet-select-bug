{
  "ssg": "hugo",
  "mode": "hosted",
  "build": {
    "install_command": "npm i",
    "build_command": "npm run build",
    "output_path": "public",
    "environment_variables": [
      {
        "key": "HUGO_CACHEDIR",
        "value": "/usr/local/__site/src/.hugo_cache/"
      }
    ],
    "preserved_paths": "resources/,.hugo_cache/,node_modules/",
    "preserve_output": false,
    "include_git": false,
    "manually_configure_urls": false,
    "hugo_version": "0.145.0",
    "ruby_version": "2.7.3",
    "node_version": "20",
    "deno_version": "1.40.2"
  }
}
